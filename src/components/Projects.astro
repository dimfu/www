---
import { Projects as _Projects} from "@/content/projects";
import ProjectItem from "./ProjectItem.astro";
---

<ul class="flex flex-col mt-2 gap-2">
   {
      _Projects.map(
         ({ name, description, url}) => (
            <ProjectItem
               title={name}
               description={description}
               href={url}
            />
         ),
      )
   }
</ul>
